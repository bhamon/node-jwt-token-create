{
    "parserOptions":{
        "ecmaVersion":8,
        "sourceType":"module",
        "ecmaFeatures":{
            "impliedStrict":true
        }
    },
    "env":{
        "node":true,
        "es6":true
    },
    "extends":"eslint:recommended",
    "rules":{
        "indent":["error", "tab", {
            "SwitchCase":1
        }],
        "semi":["error", "always"],
        "curly":["error", "all"],
        "eol-last":["error", "always"],
        "no-console":"off"
    }
}
